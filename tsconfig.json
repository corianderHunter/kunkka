{
  "compilerOptions": {
    "module": "es2015",
    "lib": ["es2015", "es2016", "es2017"],
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react-native",
    "preserveConstEnums": true,
    "sourceMap": false,
    "outDir": "./dist",
    "declaration": true,
    "skipLibCheck": true
  },
  "include": ["src/Drawer/*"],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
